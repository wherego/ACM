#include <iostream>
using namespace std;

int main() {

	int h;
	int n;
	int x;
	int t0;
	cin>>h>>n>>x>>t0;
	if(n>=x)
		cout<<"-1";
	bool flag=true;
	for(int t=0;t<t0;t++)
	{
		if(h+n*t-x*t<0)
		{
			cout<<"angerous! Be careful!";
		}

	}
	return 0;
}
