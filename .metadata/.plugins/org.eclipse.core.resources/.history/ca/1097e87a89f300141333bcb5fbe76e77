#include <iostream>
#include <math.h>
using namespace std;

int main() {

	int sum=0;
	int n;
	cin>>n;
	for(int k=1;k*(k+1)/2<=n;k++)
	{
if(k*(k+1)==n)
{cout<<k;
for(int j=1;j<=k;j++)
{sum+=j*j;}
}
else
	if(k*(k+1)/2<n&&n>k*(k-1)/2)
	{}
	}


	return 0;
}
