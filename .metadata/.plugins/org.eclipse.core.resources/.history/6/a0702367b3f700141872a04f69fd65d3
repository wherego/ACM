#include <iostream>
using namespace std;


int sum1(int a[],int len)
{
	int sum=0;
	for(int i=0;i<len;i++)
	{
		sum+=a[i];
	}

}

int sum2(int a[],int len)
{
	int sum=0;
	int i=0;
	while(i<len)
		{sum+=a[i++];}

}



int main() {

	return 0;
}
