#include <iostream>
using namespace std;

int main() {

  int n;
  int m;
  int p;
  cin>>n>>m>>p;
  int a[m];
  int b[p];
  for(int i=0;i<m;i++)
  {
  cin>>a[i];
  }
  for(int j=0;j<p;j++)
   {
cin>>b[j];

   }

	return 0;
}
